// Config for QC-only run (no generation, no eval)
params {
    mode = 'qc'
    
    // Point to existing generated sequences
    input_dir = "${projectDir}/results/generated"
    
    // Use vLLM-matching parameters for any generation (shouldn't run)
    num_samples = 50
    max_new_tokens = 256
    temperature = 0.95
    top_p = 0.90
    top_k = 0
    repetition_penalty = 1.0
    batch_size = 32
}
